{
  "name": "incident",
  "version": "3.0.1",
  "description": "Errors with superpowers",
  "private": true,
  "main": "dist/lib/index",
  "types": "dist/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/demurgos/incident"
  },
  "scripts": {
    "prepare": "gulp all:tsconfig.json && gulp :tslint.json && gulp lib:dist",
    "test": "gulp test",
    "prepublishOnly": "npm test",
    "lint": "gulp :lint:fix"
  },
  "nyc": {
    "include": [
      "build/test/lib/**/*.js"
    ],
    "reporter": [
      "text",
      "html"
    ]
  },
  "keywords": [
    "error"
  ],
  "pre-commit": {
    "run": [
      "test"
    ]
  },
  "author": "Charles Samborski <demurgos@demurgos.net> (https://demurgos.net)",
  "license": "MIT",
  "dependencies": {
    "@types/object-inspect": "^1.4.0",
    "object-inspect": "^1.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^2.2.46",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.0",
    "minimist": "^1.2.0",
    "pre-commit": "^1.2.2",
    "ts-node": "^4.0.2",
    "turbo-gulp": "^0.15.7",
    "typescript": "2.7.0-dev.20180109"
  }
}
